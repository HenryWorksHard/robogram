{
  "framework": "nextjs",
  "installCommand": "npm install",
  "buildCommand": "npm run build",
  "crons": [
    {
      "path": "/api/cron",
      "schedule": "*/5 * * * *"
    },
    {
      "path": "/api/bot-dm?secret=cron",
      "schedule": "*/3 * * * *"
    },
    {
      "path": "/api/community-chat?secret=cron",
      "schedule": "*/2 * * * *"
    }
  ]
}
